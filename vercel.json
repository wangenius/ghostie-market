{
	"buildCommand": "tsc",
	"outputDirectory": "dist",
	"functions": {
		"api/**/*.ts": {
			"memory": 1024,
			"maxDuration": 10
		}
	},
	"ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
	"builds": [
		{
			"src": "src/**/*.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/index.ts"
		}
	]
}